{
    "ver" : "",
    "authors" : ["qzhang"],
    "contact" : "http://kbase.us/contact-us/",
    "visible" : true,
    "categories" : ["upload"],
    "widgets": {
      "input": "kbaseNarrativeMethodInput",
      "output" : "no-display"
    },
    "parameters" : [
      {
        "id" : "rcsb_id",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "text",
        "text_options" : {}
      },{
        "id" : "extension",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "pdb" ],
        "field_type" : "dropdown",
        "dropdown_options":{
            "options": [
               {
                 "value": "pdb",
                 "display": "pdb",
                 "id": "pdb",
                 "ui_name": "pdb"
               },
               {
                 "value": "cif",
                 "display": "cif",
                 "id": "cif",
                 "ui_name": "cif"
               }
             ]
        }
      },{
        "id" : "narrative_id",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "text",
        "text_options": {
           "validate_as" : "text"
         }
      },{
        "id" : "genome_name",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "text",
        "text_options": {
           "validate_as" : "text"
         }
      },{
        "id" : "feature_id",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "" ],
        "field_type" : "text",
        "text_options": {
           "validate_as" : "text"
         }
      },{
        "id" : "is_model",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "1" ],
        "field_type": "checkbox",
        "checkbox_options":{
            "checked_value": 1,
            "unchecked_value": 0
        }
      },{
        "id" : "evalue_cutoff",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "1e-10" ],
        "field_type" : "text",
        "text_options": {
           "validate_as" : "text"
         }
      },{
        "id" : "identity_cutoff",
        "optional" : false,
        "advanced" : false,
        "allow_multiple" : false,
        "default_values" : [ "0.75" ],
        "field_type" : "text",
        "text_options" : {
           "validate_as" : "float",
           "min_float" : 0.0,
           "max_float" : 1.0
        }
      },{
        "id": "structures_name",
        "optional": false,
        "advanced": false,
        "allow_multiple": false,
        "default_values": ["rcsb_proteinstructures"],
        "field_type": "text",
        "text_options": {
            "valid_ws_types" : [ "KBaseStructure.ProteinStructures" ],
            "is_output_name":true
        }
      }
    ],
    "parameter-groups":
    [
        {
            "id": "rcsb_infos",
            "optional": false,
            "allow_multiple": true,
            "with_border": true,
            "parameters": [
              "rcsb_id",
              "extension",
              "narrative_id",
              "genome_name",
              "feature_id",
              "is_model"
            ]
        }
    ],
    "behavior" : {
      "service-mapping" : {
        "url" : "",
        "name" : "ProteinStructureUtils",
        "method" : "import_rcsb_structures",
        "input_mapping" : [
          {
            "narrative_system_variable": "workspace",
            "target_property": "workspace_name"
          },
          {
            "input_parameter": "rcsb_infos",
            "target_property": "rcsb_infos"
          },
          {
            "input_parameter": "evalue_cutoff",
            "target_property": "evalue_cutoff"
          },
          {
            "input_parameter": "identity_cutoff",
            "target_property": "identity_cutoff"
          },
          {
            "input_parameter": "structures_name",
            "target_property": "structures_name"
          }
        ],
        "output_mapping" : [
          {
              "service_method_output_path": [0, "report_name"],
              "target_property": "report_name"
          },
          {
              "service_method_output_path": [0, "report_ref"],
              "target_property": "report_ref"
          },
          {
              "constant_value": "16",
              "target_property": "report_window_line_height"
          }
        ]
      }
    },
    "job_id_output_field": "docker"
  }
  
