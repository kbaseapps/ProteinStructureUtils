{
  "ver" : "",
  "authors" : ["jjeffryes", "qzhang"],
  "contact" : "http://kbase.us/contact-us/",
  "visible" : true,
  "categories" : ["upload"],
  "widgets": {
    "input": "kbaseNarrativeMethodInput",
    "output" : "no-display"
  },
  "parameters" : [
    {
      "id" : "input_staging_file_path",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "dynamic_dropdown",
      "text_options" : {}
    },
    {
      "id" : "description",
      "optional" : true,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "textarea"
    },
    {
      "id" : "structure_name",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseStructure.ModelProteinStructure"],
        "is_output_name" : true
      }
    }
  ],
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "ProteinStructureUtils",
      "method" : "import_model_pdb_file",
      "input_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "input_parameter": "input_staging_file_path",
          "target_property": "input_staging_file_path"
        },
        {
          "input_parameter": "description",
          "target_property": "description"
        },
        {
          "input_parameter": "structure_name",
          "target_property": "structure_name"
        }
      ],
      "output_mapping" : [
        {
            "service_method_output_path": [0, "report_name"],
            "target_property": "report_name"
        },
        {
            "service_method_output_path": [0, "report_ref"],
            "target_property": "report_ref"
        },
        {
            "constant_value": "16",
            "target_property": "report_window_line_height"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
